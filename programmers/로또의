function solution(lottos, win_nums) {
     const answer = [];
     let winCount = 0;
     let zeroCount = 0;

     winCount = win_nums.filter((num) => lottos.includes(num)).length;
     zeroCount = lottos.filter((num) => num === 0).length;

     answer[0] = rating(winCount + zeroCount);
     answer[1] = rating(winCount);
    
     return answer;
}

function rating(n) {
  switch(n) {
    case 6 : 
      return 1
    case 5 :
      return 2
    case 4 :
      return 3
    case 3 :
      return 4 
    case 2 :
      return 5
    default :
      return 6
  }
}
